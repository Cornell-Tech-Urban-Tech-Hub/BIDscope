#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules/@esri/calcite-ui-icons/bin/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules/@esri/calcite-ui-icons/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules/@esri/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules/@esri/calcite-ui-icons/bin/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules/@esri/calcite-ui-icons/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules/@esri/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@esri+calcite-ui-icons@4.1.0/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/spriter.js" "$@"
else
  exec node  "$basedir/../../bin/spriter.js" "$@"
fi
