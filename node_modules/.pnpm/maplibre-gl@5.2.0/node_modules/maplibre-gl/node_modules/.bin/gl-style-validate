#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/maplibre-gl-style-spec/dist/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/maplibre-gl-style-spec/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/maplibre-gl-style-spec/dist/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/maplibre-gl-style-spec/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.1.0/node_modules:/Users/ninawang/Desktop/BIDspec/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/maplibre-gl-style-spec/dist/gl-style-validate.mjs" "$@"
else
  exec node  "$basedir/../../../../../@maplibre+maplibre-gl-style-spec@23.1.0/node_modules/@maplibre/maplibre-gl-style-spec/dist/gl-style-validate.mjs" "$@"
fi
